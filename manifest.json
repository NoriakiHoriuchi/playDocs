  "name": "playDocs",
  "version": "1",
  "description": "PlayFramework Documentation Switch",
  "icons": {
    "16": "images/icon/play16.png",
    "48": "images/icon/play48.png",
    "128": "images/icon/play128.png"
  },
  "browser_action": {
